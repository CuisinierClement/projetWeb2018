{
    "id":"manage-users",
    "name":"Manage Users",
    "modules":{
        "manageUsers" : {
            "path":"{manage-users}/modules/manageUsers",
            "autoload": true,
            "config": {
                "apiUrl": "${seed-rest-path-slash}"
            }
        }
    },
    "i18n": {
